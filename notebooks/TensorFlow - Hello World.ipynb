   "source": [
    "import tensorflow as tf\n",
    "\n",
    "hello = tf.Variable('Hello Distributed World!')\n",
    "\n",
    "server = tf.train.Server.create_local_server()\n",
    "sess = tf.Session(server.target)\n",
    "init = tf.global_variables_initializer()\n",
    "\n",
    "sess.run(init)\n",
    "sess.run(hello)"
   ]
  }
